#!name=中青看看赚 & 浏览赚 模块
#!desc=用于中青看点极速版的浏览赚、看看赚、时长上报等功能监听
#!author=ChatGPT
#!system=ios

[MITM]
hostname = kandian.wkandian.com, kd.youth.cn, ios.baertt.com

[Script]
# 浏览赚：任务中心 → 看看赚 → 顶部浏览赚 → 开始任务抓包
http-request ^https?://(ios\.baertt|kandian\.wkandian)\.com\/v5\/task/browse_start\.json script-path=https://raw.githubusercontent.com/XWF888/Task/Main/zqkd/youth_banner.js, requires-body=1, tag=中青浏览赚

# 看看赚：任务中心 → 看看赚 → 点击任务抓包
http-request ^https?://(ios\.baertt|kandian\.wkandian)\.com/v5/Nameless/adlickstart\.json script-path=https://raw.githubusercontent.com/XWF888/Task/Main/zqkd/youth_banner.js, requires-body=1, tag=中青看看赚

# 任务中心（添加看看赚入口）
http-response ^https://kd\.youth\.cn/WebApi/NewTaskIos/getTaskList script-path=https://raw.githubusercontent.com/XWF888/Task/Main/zqkd/youth_banner.js, requires-body=1, tag=中青看看赚入口

# 时长上报：已有阅读时长时，只保存大于 1 分钟的数据
http-request ^https?://(ios\.baertt|kandian\.wkandian)\.com/v5/user/app_stay\.json script-path=https://raw.githubusercontent.com/XWF888/Task/Main/zqkd/youth_banner.js, requires-body=1, tag=中青iOS时长数据

# （可选）定时任务示例 - 不会在 Surge 生效，仅示例保留
# cron "30 6 * * *" script-path=https://raw.githubusercontent.com/XWF888/Task/Main/zqkd/youth_banner.js, enabled=true, tag=中青看看赚
